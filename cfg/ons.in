alias g_onslaught_cp_buildhealth "cvardeprecated g_onslaught_cp_buildhealth g_ons_cp_buildhealth; g_ons_cp_buildhealth $1"
alias g_onslaught_cp_buildtime "cvardeprecated g_onslaught_cp_buildtime g_ons_cp_buildtime; g_ons_cp_buildtime $1"
alias g_onslaught_cp_regen "cvardeprecated g_onslaught_cp_regen g_ons_cp_regen; g_ons_cp_regen $1"
